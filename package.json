{
  "name": "electron-vue-rsbuild-bun",
  "version": "0.1.2",
  "main": "dist-ts/src/main.js",
  "type": "module",
  "description": "Modern Electron + Vue boilerplate using Rsbuild and Bun runtime.",
  "scripts": {
    "dev": "node scripts/dev-electron.mjs",
    "dev:web": "node scripts/dev-web.mjs",
    "dev:electron": "node scripts/dev-electron.mjs",
    "build": "node scripts/build-electron.mjs",
    "build:frontend": "node scripts/build-frontend.mjs",
    "build:electron": "node scripts/build-electron.mjs",
    "build:ts": "tsc",
    "dist": "node scripts/build-electron.mjs --package",
    "start": "node scripts/start.mjs",
    "lint": "biome check .",
    "lint:fix": "biome check --fix .",
    "format": "biome format --write ."
  },
  "author": {
    "name": "naranyala",
    "email": "user@example.com"
  },
  "build": {
    "appId": "com.some.electron.quickstart",
    "dmg": {
      "contents": [
        {
          "x": 110,
          "y": 150
        },
        {
          "x": 240,
          "y": 150,
          "type": "link",
          "path": "/Applications"
        }
      ]
    },
    "linux": {
      "category": "Utility",
      "target": [
        "AppImage",
        "deb"
      ]
    },
    "win": {
      "target": "msi"
    },
    "files": [
      "dist-ts/**/*",
      "build/**/*"
    ],
    "extraResources": [
      {
        "from": "dist-ts/",
        "to": "dist-ts/"
      }
    ],
    "extraFiles": [
      {
        "from": "dist-ts/",
        "to": "."
      }
    ]
  },
  "keywords": [
    "rsbuild",
    "vue"
  ],
  "license": "MIT",
  "dependencies": {
    "vue": "^3.5.27",
    "winbox": "^0.2.82"
  },
  "devDependencies": {
    "@biomejs/biome": "^2.3.13",
    "@rsbuild/core": "^1.7.2",
    "@rsbuild/plugin-type-check": "^1.3.3",
    "@rsbuild/plugin-vue": "^1.2.3",
    "@types/node": "^25.1.0",
    "@types/vue": "^2.0.0",
    "@vue/compiler-sfc": "^3.5.27",
    "@vue/runtime-dom": "^3.5.27",
    "concurrently": "^9.2.1",
    "electron": "^40.1.0",
    "electron-builder": "^26.4.0",
    "electron-reload": "^1.5.0",
    "entities": "^7.0.1",
    "ts-node": "^10.9.2",
    "tsc-alias": "^1.8.16",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.9.3",
    "wait-on": "^9.0.3"
  }
}
