{
  "name": "electron-vue-rsbuild-bun",
  "version": "0.1.2",
  "type": "module",
  "description": "Modern Electron + Vue boilerplate using Rsbuild and Bun runtime.",
  "scripts": {
    "dev": "node scripts/build.mjs dev",
    "dev:electron": "node scripts/build.mjs dev:electron",
    "build": "node scripts/build.mjs build",
    "build:frontend": "node scripts/build.mjs build:frontend",
    "dist": "node scripts/build.mjs build --package",
    "start": "node scripts/build.mjs start",
    "lint": "biome check .",
    "lint:fix": "biome check --fix .",
    "format": "biome format --write ."
  },
  "author": "naranyala",
  "build": {
    "appId": "com.some.electron.quickstart",
    "dmg": {
      "contents": [
        {
          "x": 110,
          "y": 150
        },
        {
          "x": 240,
          "y": 150,
          "type": "link",
          "path": "/Applications"
        }
      ]
    },
    "linux": {
      "category": "Utility",
      "target": [
        "AppImage",
        "deb"
      ]
    },
    "win": {
      "target": "msi"
    },
    "files": [
      "electron-main/**/*",
      "build/**/*"
    ]
  },
  "keywords": [
    "rsbuild",
    "vue"
  ],
  "license": "MIT",
  "dependencies": {
    "electron": "^40.1.0",
    "vue": "^3.5.27",
    "winbox": "^0.2.82"
  },
  "devDependencies": {
    "@biomejs/biome": "^2.3.13",
    "@rsbuild/core": "^1.7.2",
    "@rsbuild/plugin-type-check": "^1.3.3",
    "@rsbuild/plugin-vue": "^1.2.3",
    "@types/node": "^25.1.0",
    "@types/vue": "^2.0.0",
    "@vue/compiler-sfc": "^3.5.27",
    "@vue/runtime-dom": "^3.5.27",
    "concurrently": "^9.2.1",
    "electron-builder": "^26.4.0",
    "electron-reload": "^1.5.0",
    "entities": "^7.0.1",
    "typescript": "^5.9.3",
    "wait-on": "^9.0.3"
  }
}
